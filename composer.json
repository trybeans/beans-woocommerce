{
    "name": "trybeans/beans-woocommerce",
    "description": "Beans plugin for WooCommerce",
    "type": "wordpress-plugin",
    "homepage": "https://wordpress.org/plugins/beans-woocommerce-loyalty-rewards/",
    "license": "MIT",
    "authors": [
        {
            "name": "Beans Team",
            "email": "dev@trybeans.com",
            "homepage": "https://www.trybeans.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "hello@trybeans.com",
        "docs": "https://help.trybeans.com/"
    },
    "config": {
        "platform": {
            "php": "7.1.0"
        }
    },
    "repositories":[
        {
            "type":"composer",
            "url":"https://wpackagist.org",
            "only": [
                "wpackagist-plugin/*",
                "wpackagist-theme/*"
            ]
        }
    ],
    "require-dev": {
        "composer/installers": "^1.11.0",
        "johnpbloch/wordpress": "~5.5.0",
        "wpackagist-plugin/woocommerce": "~4.1.0",
        "phpstan/phpstan": "^0.12.94",
        "squizlabs/php_codesniffer": "^3.6.0",
        "wp-coding-standards/wpcs": "^2.3.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
        "szepeviktor/phpstan-wordpress": "^0.7.7"
    },
    "scripts": {
        "lint": [
            "./vendor/bin/phpcs",
            "./vendor/bin/phpstan analyze --memory-limit=100"
        ],
        "test": [
            ""
        ]
    },
    "scripts-descriptions": {
        "lint": "Run all linters!",
        "test": "Run all tests!"
    },
    "extra": {
        "wordpress-install-dir": "wp",
        "installer-paths": {
            "wp/wp-content/plugins/{$name}/": [
                "type:wordpress-plugin"
            ]
        }
    }
}
